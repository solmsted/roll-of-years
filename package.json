{
    "author": "Steven Olmsted <steven.olm@gmail.com>",
    "babel": {
        "env": {
            "build": {
                "presets": [
                    "babel-preset-minify"
                ]
            },
            "test": {
                "plugins": [
                    "babel-plugin-istanbul"
                ]
            }
        },
        "plugins": [
            "babel-plugin-transform-line"
        ],
        "presets": [
            [
                "@babel/preset-env",
                {
                    "targets": {
                        "node": "current"
                    }
                }
            ]
        ],
        "sourceMaps": "inline"
    },
    "bugs": "https://github.com/solmsted/roll-of-years/issues",
    "description": "A comprehensive list of the names of the years",
    "devDependencies": {
        "@babel/cli": "~7.11.6",
        "@babel/core": "~7.11.6",
        "babel-plugin-istanbul": "~6.0.0",
        "babel-plugin-transform-line": "~0.9.0",
        "@babel/preset-env": "~7.11.5",
        "babel-preset-minify": "~0.5.1",
        "@babel/register": "~7.11.5",
        "chai": "~4.2.0",
        "cross-env": "~7.0.2",
        "eslint": "~7.5.0",
        "eslint-plugin-isotropic": "~0.6.0",
        "mocha": "~8.1.3",
        "nyc": "~15.1.0"
    },
    "engines": {
        "node": "^12.19.0",
        "npm": "^6.14.8"
    },
    "eslintConfig": {
        "env": {
            "es6": true
        },
        "extends": "plugin:isotropic/isotropic",
        "parserOptions": {
            "ecmaFeatures": {
                "globalReturn": false,
                "impliedStrict": true,
                "jsx": false
            },
            "ecmaVersion": 2020,
            "sourceType": "module"
        },
        "plugins": [
            "isotropic"
        ],
        "root": true
    },
    "files": [
        "lib"
    ],
    "homepage": "https://github.com/solmsted/roll-of-years",
    "keywords": [
        "alaundo",
        "alaundo the seer",
        "augathra",
        "augathra the mad",
        "calendar",
        "calendar of harptos",
        "candlekeep",
        "d&d",
        "dalereckoning",
        "dnd",
        "dr",
        "dragon",
        "dragons",
        "dungeon",
        "dungeons",
        "dungeons & dragons",
        "dungeons and dragons",
        "faerun",
        "faer√ªn",
        "forgotten realms",
        "harptos",
        "lost sage",
        "netheril",
        "year",
        "years"
    ],
    "license": "CC-BY-SA-3.0",
    "main": "lib/roll-of-years.js",
    "name": "roll-of-years",
    "nyc": {
        "branches": 100,
        "check-coverage": false,
        "functions": 100,
        "instrument": false,
        "lines": 100,
        "per-file": false,
        "reporter": [
            "lcov",
            "text-summary"
        ],
        "require": [
            "@babel/register"
        ],
        "sourceMap": false,
        "statements": 100
    },
    "repository": "solmsted/roll-of-years",
    "scripts": {
        "build": "cross-env BABEL_ENV=build babel js -d lib",
        "lint": "eslint js test",
        "posttest": "[ -z \"$npm_config_coverage\" ] || nyc check-coverage",
        "prepare": "npm run build",
        "prepublishOnly": "npm test --coverage",
        "pretest": "npm run lint",
        "test": "cross-env BABEL_ENV=test nyc mocha"
    },
    "version": "0.0.1"
}
